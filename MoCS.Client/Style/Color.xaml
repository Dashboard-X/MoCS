<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color x:Key="BaseLightColor">#FFFFFFEB</Color>
    <Color x:Key="BaseMediumColor">#FFCCB596</Color>
    <Color x:Key="BaseDarkColor">#FF36312D</Color>
    <Color x:Key="ContrastColor">#FF1761A8</Color>
    <Color x:Key="HighlightColor">#FFB30B1A</Color>

    <Color x:Key="ShadowColor">Black</Color>

    <Color x:Key="AssignmentStartedColor">LightGreen</Color>
    <Color x:Key="AssignmentNotStartedColor">LightBlue</Color>
    <Color x:Key="AssignmentNotActiveColor">LightGray</Color>
</ResourceDictionary>